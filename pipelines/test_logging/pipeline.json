{
  "enabled": true,
  "description": "Test-Pipeline für Logging und Frontend-Tests - wartet 20 Sekunden und generiert regelmäßig Logs",
  "tags": ["test", "logging", "frontend-test", "demo"],
  "timeout": 30,
  "retry_attempts": 1,
  "cpu_hard_limit":2,
  "mem_hard_limit": "256M",
  "cpu_soft_limit": 1,
  "mem_soft_limit": "200M",
  "default_env": {
    "TEST_ENV_VAR": "logging_test_value",
    "LOG_LEVEL": "INFO",
    "CUSTOM_VAR": "custom_test_value",
    "ENABLE_LOGGING": "true"
  }
}
